[access]
	inheritFrom = All-Projects
[access "refs/*"]
	owner = group user/Rémi Benoit (AltarBeastiful)
[access "refs/for/refs/*"]
	read = group user/Rémi Benoit (AltarBeastiful)
	push = group user/Rémi Benoit (AltarBeastiful)
	label-Code-Review = -2..+2 group user/Rémi Benoit (AltarBeastiful)
	label-Verified = -1..+1 group user/Rémi Benoit (AltarBeastiful)
	rebase = group user/Rémi Benoit (AltarBeastiful)
	removeReviewer = group user/Rémi Benoit (AltarBeastiful)
	submit = group user/Rémi Benoit (AltarBeastiful)
[access "refs/heads/*"]
	read = group user/Rémi Benoit (AltarBeastiful)
	create = group user/Rémi Benoit (AltarBeastiful)
	push = +force group user/Rémi Benoit (AltarBeastiful)
	pushMerge = group user/Rémi Benoit (AltarBeastiful)
[access "refs/tags/*"]
	push = +force group user/Rémi Benoit (AltarBeastiful)
	createTag = group user/Rémi Benoit (AltarBeastiful)
	createSignedTag = group user/Rémi Benoit (AltarBeastiful)
